{
  "name": "fqm-execution",
  "version": "0.2.1",
  "description": "FHIR Quality Measure Execution",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "bin": {
    "fqm-execution": "build/cli.js"
  },
  "files": [
    "build/*"
  ],
  "dependencies": {
    "@ahryman40k/ts-fhir-types": "^4.0.32",
    "atob": "^2.1.2",
    "commander": "^6.1.0",
    "cql-exec-fhir": "1.5.0-beta.1",
    "cql-execution": "^2.2.0",
    "handlebars": "^4.7.6",
    "moment": "^2.29.0",
    "ts-node": "9.1.1",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@types/handlebars": "^4.1.0",
    "@types/jest": "^26.0.5",
    "@types/node": "^14.0.23",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "cql-translation-service-client": "^0.6.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "shx": "^0.3.3",
    "ts-jest": "^26.1.3",
    "ts-node": "^9.1.1",
    "typescript": "^3.9.7"
  },
  "scripts": {
    "build": "shx rm -rf ./build && tsc",
    "build:watch": "shx rm -rf ./build && tsc -w",
    "lint": "tsc && eslint \"**/*.{js,ts}\"",
    "lint:fix": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "prettier": "prettier --check \"**/*.{js,ts}\"",
    "prettier:fix": "prettier --write \"**/*.{js,ts}\"",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:integration": "ts-node --files ./src/_tests_/run_tests.ts",
    "check": "npm run test && npm run lint && npm run prettier",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/projecttacoma/fqm-execution.git"
  },
